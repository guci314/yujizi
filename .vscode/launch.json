{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "attach",
        "name": "Attach to Chrom",
        "port": 9222,
        "urlFilter": "http://localhost:5500/*",
        "webRoot": "${workspaceFolder}"
        // "runtimeExecutable": "/opt/google/chrome/google-chrome"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch chrome",
        "url": "http://localhost:5500",
         "runtimeExecutable": "/usr/bin/google-chrome-stable"

      },
      {
        "type": "msedge",
        "request": "launch",
        "name": "Launch in edge",
        "url": "http://localhost:5500"
      }
    ]
  }